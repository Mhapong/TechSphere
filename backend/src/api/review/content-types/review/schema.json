{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "star": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "required": true
    },
    "comment": {
      "type": "text",
      "required": false
    },
    "users_review": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "course_review"
    },
    "review_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "rating"
    }
  }
}
