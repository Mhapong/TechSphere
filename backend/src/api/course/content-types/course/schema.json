{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "course_id": {
      "type": "biginteger"
    },
    "Name": {
      "type": "string"
    },
    "Price": {
      "type": "decimal"
    },
    "Description": {
      "type": "text"
    },
    "Time_Usage": {
      "type": "integer"
    },
    "create_date": {
      "type": "datetime"
    },
    "rating": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "review_id"
    },
    "topic": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::topic.topic",
      "mappedBy": "topic_id"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "category_ids"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "course_owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "owned_course"
    }
  }
}
